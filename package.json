{
  "name": "libs-monorepo-template",
  "version": "1.0.0",
  "description": "A monorepo template for libraries",
  "author": "Jamie-Yang",
  "private": true,
  "license": "ISC",
  "type": "module",
  "scripts": {
    "lint": "run-p lint:prettier lint:eslint lint:stylelint",
    "lint:prettier": "prettier . --write --log-level warn",
    "lint:eslint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts --fix --ignore-path .gitignore",
    "lint:stylelint": "stylelint \"./packages/**/*.{vue,scss,css}\" --fix --ignore-path .gitignore",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "cli": "workspace:*",
    "vue": "^3.4.30"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.26.0",
    "execa": "^9.3.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "npm-run-all": "^4.1.5",
    "ora": "^8.0.1",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "stylelint": "^16.6.1",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.22"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.12.0",
    "pnpm": ">=9.4.0"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
