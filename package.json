{
  "name": "libs-monorepo-template",
  "version": "1.0.0",
  "description": "A monorepo template for libraries",
  "author": "Jamie-Yang",
  "private": true,
  "license": "ISC",
  "type": "module",
  "scripts": {
    "lint": "run-p lint:prettier lint:eslint lint:stylelint",
    "lint:prettier": "prettier . --write --log-level warn",
    "lint:eslint": "eslint . --fix",
    "lint:stylelint": "stylelint \"./apps/**/*.{vue,scss,css}\" --fix --ignore-path .gitignore",
    "lint:lint-staged": "lint-staged",
    "postinstall": "pnpm -r run stub --if-present",
    "prepare": "husky"
  },
  "dependencies": {
    "cli": "workspace:*",
    "vue": "^3.5.27"
  },
  "devDependencies": {
    "@internals/commitlint-config": "workspace:*",
    "@internals/eslint-config": "workspace:*",
    "@internals/lint-staged-config": "workspace:*",
    "@internals/prettier-config": "workspace:*",
    "@internals/stylelint-config": "workspace:*",
    "@internals/tsconfig": "workspace:*",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.4"
  },
  "engines": {
    "node": ">=20.12.0",
    "pnpm": ">=9.4.0"
  },
  "packageManager": "pnpm@10.28.2"
}
